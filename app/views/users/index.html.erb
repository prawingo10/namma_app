<h1>this is user list</h1>


<table>
<thead>
<tr><th>Name:</th>
<th>Email:</th></tr>

</thead>
<tbody>
<% @user.each do |user|%>

<tr>
<td><%= user.username%></td>
<td><%= user.email%></td>
<td><%= link_to 'SHOW', user_path(user) %></td>
<td><%= link_to 'EDIT', edit_user_path(user) %></td>
<td><%= link_to 'DELETE', user_path(user),method: :delete %></td>
</tr>

</tbody>
<% end %>

</table>



